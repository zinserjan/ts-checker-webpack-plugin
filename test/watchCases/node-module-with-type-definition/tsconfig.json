{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  },
  "include": [
    "src",
    "typePatches/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": [
    "typePatches"
  ]
}
